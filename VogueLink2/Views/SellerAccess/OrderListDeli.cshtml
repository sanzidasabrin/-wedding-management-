@model IEnumerable<VogueLink2.Models.ProductOrder>

@{
    ViewBag.Title = "OrderListDeli";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Delivered Order List</h2>

<h4>Delivered Product <br /> @ViewBag.con</h4>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Product.Product_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Order_Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Order_Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Order_Size)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Product.Product_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Order_Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Order_Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Order_Size)
            </td>
            <td>
                @Html.ActionLink("Done", "OrderDone", new { id = item.PO_Id }) |

            </td>
        </tr>
    }

</table>
